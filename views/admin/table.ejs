                                                              <th>constituency</th>
                                                              <th>Ward</th>
                                                              <th>Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                          <% for(i=0; i<users.length; i++) { %>
                                                          <% if(users[i].post=="Governor") { %>
                                                          <tr>

                                                              <td><%= users[i].firstname %></td>
                                                              <td><%= users[i].lastname %></td>
                                                              <td><%= users[i].email %></td>
                                                              <td><%= users[i].phoneno %></td>
